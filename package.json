{
	"name": "genshin-db",
	"version": "5.1.8",
	"description": "Genshin Impact v5.0 JSON data. Search and get results in all in-game languages! Sources from the fandom wiki and GenshinData repo.",
	"main": "./src/main.js",
	"types": "./types/index.d.ts",
	"scripts": {
		"examples": "cd scripts/generate && node ./examples.js",
		"makeIndex": "cd scripts/generate && node ./makeIndex.js",
		"combineData": "cd scripts/generate && node ./combineData.js",
		"imagelist": "cd scripts/imagelist && node ./images.js",
		"assemble": "npm run makeIndex && npm run combineData && npm run examples && npm run imagelist",
		
		"build": "node ./scripts/webpack/build.js",
		"buildAllDist": "node ./scripts/webpack/buildAllDist.js",

		"publish": "node ./scripts/publish/publish.js",
		"publishTcg": "npm run publish tcg"
	},
	"keywords": [
		"genshin-impact",
		"genshinimpact",
		"genshin"
	],
	"author": "thebowja",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/theBowja/genshin-db.git"
	},
	"license": "MIT",
	"dependencies": {
		"fuzzysort": "^1.1.4",
		"pako": "^2.0.4"
	},
	"devDependencies": {
		"arraybuffer-loader": "^1.0.8",
		"follow-redirects": "^1.14.9",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.2.0",
		"yargs-parser": "^21.1.1"
	},
	"files": [
		"examples/EXAMPLES.md",
		"examples/index.ts",
		"LICENSE",
		"README.md",
		"types/*",
		"src/altnames.js",
		"src/constants.js",
		"src/language.js",
		"src/folder.js",
		"src/helper.js",
		"src/main.js",
		"src/getdata.js",
		"src/Options.js",
		"src/min/data.min.json",
		"src/design.json",
		"src/v4prop.js"
	]
}
