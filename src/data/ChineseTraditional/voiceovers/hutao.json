{
	"id": 10000046,
	"name": "Hu Tao",
	"friendLines": [
		{
			"voicelineId": 36009,
			"title": "初次見面…",
			"voicelineType": "1000",
			"description": "喲，找本堂主有何貴幹呀？嗯？{F#妳}{M#你}不知道嗎，往生堂第七十七代堂主就是胡桃我啦！嘶，不過瞧{F#妳}{M#你}的模樣，容光煥發、身體健康，嗯…想必是為了工作以外的事來找我，對吧？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_draw_appear"
		},
		{
			"voicelineId": 36010,
			"title": "閒聊·往生堂",
			"voicelineType": "1101",
			"description": "不如去我那喝杯茶？嘿~",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_explore_idle_01"
		},
		{
			"voicelineId": 36011,
			"title": "閒聊·客戶",
			"voicelineType": "1102",
			"description": "一個客戶，兩個客戶，三個客戶…",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_explore_idle_02"
		},
		{
			"voicelineId": 36012,
			"title": "閒聊·偷懶",
			"voicelineType": "1103",
			"description": "咳咳…太陽出來我曬太陽，月亮出來我曬月亮囉~",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_explore_idle_03"
		},
		{
			"voicelineId": 36013,
			"title": "下雨的時候…",
			"voicelineType": "1201",
			"description": "哎呀…麻煩囉。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_heavyRain_01"
		},
		{
			"voicelineId": 36014,
			"title": "打雷的時候…",
			"voicelineType": "1202",
			"description": "打雷啦，起身啦。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_thunder_01"
		},
		{
			"voicelineId": 36015,
			"title": "下雪的時候…",
			"voicelineType": "1203",
			"description": "哇，是雪！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_snowy_01"
		},
		{
			"voicelineId": 36016,
			"title": "起風的時候…",
			"voicelineType": "1204",
			"description": "嗯，真舒服。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_breeze_01"
		},
		{
			"voicelineId": 36017,
			"title": "颳大風了…",
			"voicelineType": "1205",
			"description": "欸？…唉，帽子都要吹飛了。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_gale_01"
		},
		{
			"voicelineId": 36018,
			"title": "早安…",
			"voicelineType": "1001",
			"description": "嗯~早起身體好，晚睡人會飄。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_greetingMorning"
		},
		{
			"voicelineId": 36019,
			"title": "午安…",
			"voicelineType": "1002",
			"description": "喲！中午好呀，吃了嗎？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_greetingNoon"
		},
		{
			"voicelineId": 36020,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "嘻嘻，月亮出來囉~咱們也出門吧。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_greetingNight"
		},
		{
			"voicelineId": 36021,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "哦，睏了嗎？那{F#妳}{M#你}好好休息，我一個人四處轉轉。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_greetingNight2"
		},
		{
			"voicelineId": 36022,
			"title": "關於胡桃自己·名字",
			"voicelineType": "2001",
			"description": "胡桃的胡是胡吃海喝的胡，胡桃的桃卻不是淘氣的淘！嘿嘿…不、不好笑嗎？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_idle"
		},
		{
			"voicelineId": 36023,
			"title": "關於胡桃自己·拿手好戲",
			"voicelineType": "2002",
			"description": "變個戲法給{F#妳}{M#你}看，瞧好了哦…火！然後…嘩！火蝴蝶！呼，飛啦——哈哈。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_idle2"
		},
		{
			"voicelineId": 36024,
			"title": "關於我們·好奇心",
			"voicelineType": "2003",
			"description": "{F#妳}{M#你}到處旅行，一定見多識廣。有空也給我講講{F#妳}{M#你}自己的故事吧。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_close"
		},
		{
			"voicelineId": 36025,
			"title": "關於我們·夜晚",
			"voicelineType": "2004",
			"description": "白天{F#妳}{M#你}儘管到處跑，晚上可得小心點。我不在的時候，務必謹慎行動。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_close1",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到6後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 36026,
			"title": "關於我們·幫手",
			"voicelineType": "2005",
			"description": "需要幫手嗎？需要幫手嗎？我來啦！若{F#妳}{M#你}需要幫助，胡桃我定當全力以赴，絕不推辭。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_close2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到6後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 36027,
			"title": "關於我們·戰鬥",
			"voicelineType": "2006",
			"description": "戰鬥是件麻煩事，對我來說，它不是目的而是手段。用手段來達到目的，用戰鬥來維護不願讓步的東西。就這方面而言，我和{F#妳}{M#你}是一樣的。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_close3",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到6後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 36028,
			"title": "關於「神之眼」…",
			"voicelineType": "3001",
			"description": "「神之眼」…「神之眼」？唔，這東西，隨緣就好。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36029,
			"title": "有什麼想要分享…",
			"voicelineType": "3002",
			"description": "受人之託，忠人之事。我們往生堂性質特殊，肩負著雙倍責任，所以，一定要讓兩個世界的人都滿意才行。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_share_01"
		},
		{
			"voicelineId": 36030,
			"title": "感興趣的見聞…",
			"voicelineType": "3003",
			"description": "有些花草只長在陰氣重月光多的地方，{F#妳}{M#你}見過嗎？沒有？那下次帶{F#妳}{M#你}見識見識。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_pref_gift"
		},
		{
			"voicelineId": 36031,
			"title": "關於鍾離…",
			"voicelineType": "4001",
			"description": "鍾離長得那麼年輕，性格倒像是老古董，什麼都懂，卻什麼都不關心，他該不會…？嘻嘻…\n唉，算了，隨他去吧。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_zhongli_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36032,
			"title": "關於七七…",
			"voicelineType": "4002",
			"description": "看到七七了嗎？快告訴我她在哪，我要把她藏起來，嘿！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_qiqi_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36033,
			"title": "關於白朮…",
			"voicelineType": "4003",
			"description": "白朮？哼，麻煩的傢伙。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_baizhu_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36034,
			"title": "關於凝光…",
			"voicelineType": "4004",
			"description": "天權凝光，富甲一方；明眸善睞，桃羞杏讓；目達耳通，百了千當！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_ningguang_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36035,
			"title": "關於北斗…",
			"voicelineType": "4005",
			"description": "北斗姐可是璃月名人，她不認識我沒關係，我認識她就行。啊~真想和她交個朋友啊。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_beidou_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36036,
			"title": "關於行秋…",
			"voicelineType": "4006",
			"description": "聽說行秋少爺在寫書？話本還是怪談呢？好想把平日遇見的雜事說給他聽，寫成書一定很好看吧。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_xingqiu_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36037,
			"title": "關於重雲…",
			"voicelineType": "4007",
			"description": "純陽正氣，陰陽合一？嘖嘖，嘶…沒想到世上真有這種體質的人。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_chongyun_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36038,
			"title": "關於魈…",
			"voicelineType": "4008",
			"description": "仙家的事，我怎麼知道？哎呀呀，天機不可洩露。{F#妳}{M#你}就別問我啦。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_xiao_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36039,
			"title": "關於刻晴…",
			"voicelineType": "4009",
			"description": "「玉衡」刻晴啊，嘶…是個很難打交道的人呢。嘿嘿，瞧那一本正經的樣子，真想逗逗她。哎呦，她應該不會用劍敲我頭吧？不會的吧？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_keqing_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36040,
			"title": "關於香菱…",
			"voicelineType": "4010",
			"description": "香菱可好玩了，一碰就跳，特別容易被唬住。做我們這行，最喜歡這種人囉。不過也得注意分寸，畢竟，鍋巴會替香菱出頭呢。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_xiangling_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36041,
			"title": "關於雲堇…",
			"voicelineType": "4011",
			"description": "雲堇她在正式場合很優雅，私底下卻非常容易親近哦！上次我跟她打了個賭，輸的人要用自己習慣的唱法唱對方填的詞。哎呀呀，戲腔版的丘丘謠，{F#妳}{M#你}真該聽一次！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_yunjin_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36042,
			"title": "關於申鶴…",
			"voicelineType": "4012",
			"description": "她可真是個有意思的人哪，跟別人都不一樣。{F#妳}{M#你}說，要是我從現在開始天天吃仙草、喝山露，會不會也變成她那麼不染紅塵的樣子？嘿嘿…",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_shenhe_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36085,
			"title": "關於藍硯…",
			"voicelineType": "4013",
			"description": "藍姑娘啊…她實力不俗，可不僅僅是「皮毛」這麼簡單。奇門秘術可真了不得，病氣死氣都被搬走啦。哎呀，可謂是——世風日上，身強力壯，往生堂的生意越來越難做囉。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_lanyan_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36043,
			"title": "想要瞭解胡桃·其一",
			"voicelineType": "5001",
			"description": "大丘丘病了，二丘丘瞧；三丘丘採藥，四丘丘熬；五丘丘死了，六丘丘抬~\n欸…呼。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_01"
		},
		{
			"voicelineId": 36044,
			"title": "想要瞭解胡桃·其二",
			"voicelineType": "5002",
			"description": "小巷派暗黑打油詩人，正是本人！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到3後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 36045,
			"title": "想要瞭解胡桃·其三",
			"voicelineType": "5003",
			"description": "如果要我說，往生堂的工作既不困難也不可怕，只是麻煩而已。可惜旁人理解不了，見到咱們夜裡出工，總是有些懼怕。唉，是他們不懂啊。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36046,
			"title": "想要瞭解胡桃·其四",
			"voicelineType": "5004",
			"description": "我是「往生堂」第七十七代堂主，我爺爺是第七十五代。我經手的第一份工作，就是替爺爺辦葬禮。是不是很意外啊？哎呀，不知不覺就在這行做了好些年，時間過得可真快。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到5後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				}
			]
		},
		{
			"voicelineId": 36047,
			"title": "想要瞭解胡桃·其五",
			"voicelineType": "5005",
			"description": "陰陽有序，命運無常。死亡難以預測，卻也有它的規矩。記住了，無論何時何地，無論有何原因，人都不該輕易挑釁「死」之一事。唯有認識它、尊重它，才能明白活著的價值。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_05",
			"hasUnlockConditions": true,
